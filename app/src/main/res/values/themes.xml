<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WebviewDialog" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Status bar color. -->
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.WebviewDialog.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.WebviewDialog.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.WebviewDialog.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>